<div class="container">
    <h2>User List</h2>
    <table id="userTable" class="table table-hover table-striped">
        <thead>
            <th>Id</th>
            <th>Username (email)</th>
        </thead>
        <tbody>
            <% for(var i=0; i<users.length; i++) {%>
                <tr>
                    <td><%= users[i]._id %></td>
                    <td><%= users[i].email %></td>
                </tr>                            
            <% } %>
        </tbody>
    </table>                       
</div>
<script type="text/javascript" src="/js/userTable.js"></script>      
    